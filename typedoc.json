{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["src/index.ts"],
  "entryPointStrategy": "expand",
  "out": "docs",
  "exclude": [
    "src/__tests__/**/*",
    "src/**/*.test.ts",
    "src/**/*.spec.ts",
    "src/cli/**/*"
  ],
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "excludeNotDocumented": false,
  "excludeExternals": true,
  "excludeReferences": false,
  "includeVersion": false,
  "categorizeByGroup": true,
  "sort": ["source-order", "required-first", "kind"],
  "kindSortOrder": ["Function", "Class", "Interface", "TypeAlias", "Variable"],
  "groupOrder": [
    "Core Operations",
    "Builders",
    "Core Types",
    "Query & Filter",
    "Insights",
    "Merge",
    "Schema & Versioning",
    "Type Guards",
    "ID Generation",
    "Core Options",
    "Builder Options",
    "Query & Filter Options",
    "Insights Options",
    "Merge Options",
    "Validation Options",
    "Errors",
    "Constants",
    "*"
  ],
  "readme": "API.md",
  "useFirstParagraphOfCommentAsSummary": true,
  "plugin": ["typedoc-plugin-markdown"],
  "name": "CTRF",
  "githubPages": false,
  "gitRemote": "origin",
  "gitRevision": "main",
  "cleanOutputDir": true,
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": false
  }
}
